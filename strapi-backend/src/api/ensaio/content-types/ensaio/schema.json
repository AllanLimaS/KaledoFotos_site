{
  "kind": "collectionType",
  "collectionName": "ensaios",
  "info": {
    "singularName": "ensaio",
    "pluralName": "ensaios",
    "displayName": "Ensaio"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Titulo": {
      "type": "string",
      "default": "Ensaio ####",
      "required": true
    },
    "Descricao": {
      "type": "text"
    },
    "link_da_pagina": {
      "type": "string",
      "required": true,
      "unique": true,
      "regex": "^[a-z0-9]+(?:-[a-z0-9]+)*$"
    },
    "Foto_capa": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "Fotos": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images",
        "files"
      ]
    }
  }
}
